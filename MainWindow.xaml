<Window x:Class="dataSync.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:dataSync"
        mc:Ignorable="d"
        Closing="Window_Closing"
        Title="珠宝管理软件启动器" Height="80" Width="300">
    <StackPanel Margin="5,0" Height="30">
        <TextBlock Name="prompt" />
        <ProgressBar Name="pbBar" Height="8" Margin="0,5,0,0" Minimum="0" Maximum="100"></ProgressBar>
    </StackPanel>
</Window>
