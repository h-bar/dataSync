<Window x:Class="dataSync.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:dataSync"
        mc:Ignorable="d"
        Title="珠宝管理软件启动器" Height="80" Width="320">
    <StackPanel>
        <TextBlock Name="prompt" Text="系统启动中。。。请不要强制停止或关机" />
        <ProgressBar IsIndeterminate="True" Name="pBar" Margin="0,5,0,0"/>
    </StackPanel>
</Window>
